<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.smueleven">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.Smueleven">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>

        <activity android:name=".RegisterActivity"/>
        <activity android:name=".ScanActivity"/>
        <activity android:name=".CustomScannerActivity"/>
        <activity android:name=".AddActivity"/>
        <activity android:name=".ManagementActivity"/>
        <activity android:name=".SellActivity"/>
        <activity android:name=".SellScanActivity"/>
    </application>

</manifest>
